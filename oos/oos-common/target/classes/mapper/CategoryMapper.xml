<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.oos.mapper.CategoryMapper">


	 <insert id="insert">
	 	insert into tbl_category(catename,pno)
	 	values(#{catename},#{pno})
	  </insert>
	 
	  <update id="delete">
	 	update tbl_category 
	 	set del='Y'
	 	where cateno=#{cateno}
	 </update>
	                                                                                         
</mapper>